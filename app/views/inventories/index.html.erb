<table>
	<tr>
		<th> Base Price </th>
		<th> sale price </th>
		<th> offer </th>
		<th> status </th>
		<th> Action </th>
	</tr>
	<% @inventories.each do |inventory|%>
	<tr>
		<td> <%= inventory.bprice %> </td>
		<td> <%= inventory.sprice %> </td>
		<td> <%= inventory.offer %> </td>
		<td> <%= inventory.sku %> </td>
		<td> <%= link_to 'view', inventory_path(inventory) %> |<%= link_to 'Delete', inventory_path(inventory),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
	</tr>
	<% end %>
</table>